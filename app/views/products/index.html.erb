<h1>Products#index</h1>

<table>
    <thead>
        <tr>
            <th>Nome:</th>
            <th>Valor:</th>
            <th>Restaurante:</th>
        </tr>
        <tbody>
            <% @products.each do | p | %>
                <tr>
                    <th><%= p.name %></th>
                    <th><%= p.value %></th>
                    <th><%= p.restaurant.name %></th>
                </tr>
            <% end %>
        </tbody>
    </thead>
</table>
